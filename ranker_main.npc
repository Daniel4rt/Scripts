									mes "El 3er lugar recibe semanalmente:";
									for(.@i = 0; .@i < getarraysize(.rew_we_3rd); .@i++)
										mes " - "+.can_we_3rd[.@i]+"x <ITEM>"+getitemname(.rew_we_3rd[.@i])+"<INFO>"+.rew_we_3rd[.@i]+"</INFO></ITEM>";
									next;
