										mes " - "+.can_da_3rd[.@i]+"x <ITEM>"+getitemname(.rew_da_3rd[.@i])+"<INFO>"+.rew_da_3rd[.@i]+"</INFO></ITEM>";
									next;
									break;
								case 2:
									mes "[^A020F0Ranker^000000]";
									mes "El 1er lugar recibe semanalmente:";
									for(.@i = 0; .@i < getarraysize(.rew_we); .@i++)
										mes " - "+.can_we[.@i]+"x <ITEM>"+getitemname(.rew_we[.@i])+"<INFO>"+.rew_we[.@i]+"</INFO></ITEM>";
